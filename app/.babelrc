{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    "stage-3",
    "react"
  ],
  "plugins": ["react-hot-loader/babel", "transform-class-properties"],
  "env": {
    "test": {
      "presets": ["env", "stage-3", "react"],
      "plugins": ["transform-class-properties"]
    }
  }
}
